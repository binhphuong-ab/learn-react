{
  "name": "vacuum-pump-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web",
    "dev:admin": "turbo dev --filter=admin",
    "dev:api": "turbo dev --filter=api",
    "build": "turbo build",
    "build:web": "turbo build --filter=web",
    "lint": "turbo lint",
    "export": "tsx scripts/export-data.ts && turbo build --filter=web",
    "db:seed": "tsx scripts/seed-data.ts"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18"
  }
}
